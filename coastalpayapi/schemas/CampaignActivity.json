      "example": 1,
      "type": "integer",
      "format": "int32"
    },
    "newValue": {
      "description": "New value of activity",
      "example": "Test",
      "type": "string"
    },
    "assignedBy": {
      "description": "User Id",
      "example": 1,
      "type": "integer",
      "format": "int32"
    },
    "assignedAt": {
      "description": "Assigned date (Y-m-d\\TH:i:sP)",
      "example": "2019-01-01T12:04:40+05:00",
      "type": "string",
      "format": "date-time"
    }
  }
}
